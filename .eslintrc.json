{
  "rules": {
    "indent": [ 2, 2, {"VariableDeclarator": 2} ],
    "quotes": [ 2, "single" ],
    "linebreak-style": [ 2, "unix" ],
    "semi": [ 2, "always" ],
    "no-unused-vars": [ 0 ]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "require": true
  },
  "extends": "eslint:recommended"
}
